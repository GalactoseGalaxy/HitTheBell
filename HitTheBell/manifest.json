{
  "manifest_version": 3,
  "name": "HitTheBell",
  "version": "1.0",
  "description": "Notifies you when a youtuber posts a video",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  }
}